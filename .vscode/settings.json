{
    "nrf-connect.applications": [
        "${workspaceFolder}/app",
        "${workspaceFolder}\\app"
    ],
    "nrf-connect.boardRoots": [
        "${workspaceFolder}/app",
        "${workspaceFolder}\\app",
        "${workspaceFolder}/zephyr",
        "${workspaceFolder}\\zephyr",
    ],
    "nrf-connect.debugging.bindings": {
        "${workspaceFolder}/app/build": "Launch build"
    },
    "editor.defaultFormatter": "chiehyu.vscode-astyle",
    "astyle.astylerc": "${workspaceRoot}/app/tools/astyle.cfg",
    "C_Cpp.configurationWarnings": "disabled",
    "terminal.integrated.env.linux": {
        "GTK_PATH": ""
    },
    "search.useIgnoreFiles": false,
    "[c]": {
        "editor.defaultFormatter": "chiehyu.vscode-astyle"
    },
    "files.associations": {
        "log.h": "c",
        "zsw_filesystem.h": "c",
        "flash_map.h": "c",
        "chrono": "c",
        "variant": "c",
        "source_location": "c",
        "random": "c",
        "update_ui.h": "c",
        "image.h": "c",
        "img_mgmt_callbacks.h": "c",
        "smp_bt.h": "c",
        "img_mgmt.h": "c",
        "bootmode.h": "c",
        "sstream": "c",
        "regex": "c"
    }
}
